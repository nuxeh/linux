#include "tegra210-jetson-cv-super-module-e2614-base.dtsi"

/ {

	i2c@7000c000 {
		i2cmux@70 {
			compatible = "nxp,pca9546";
			reg = <0x70>;
			#address-cells = <1>;
			#size-cells = <0>;
			vcc-supply = <&vdd_3v3>;
			i2c@0 {
				reg = <0>;
				i2c-mux,deselect-on-exit;
				#address-cells = <1>;
				#size-cells = <0>;
			};
			i2c@1 {
				reg = <1>;
				i2c-mux,deselect-on-exit;
				#address-cells = <1>;
				#size-cells = <0>;
			};
			i2c@2 {
				reg = <2>;
				i2c-mux,deselect-on-exit;
				#address-cells = <1>;
				#size-cells = <0>;
			};
			i2c@3 {
				reg = <3>;
				i2c-mux,deselect-on-exit;
				#address-cells = <1>;
				#size-cells = <0>;
				rt5658: rt5659.10-001a@1a {
					compatible = "realtek,rt5658";
					reg = <0x1a>;
				};
			};
		};
	};

	/* extended sound node for superIO */
	sound {
		nvidia,dai-link-1 {
			codec-dai = <&rt5658>;
		};
	};
};
